@inbook{ahlstromvij2016,
	abstract = {Summary Information markets are markets for trading contracts representing bets on future or otherwise unknown events. Such markets generate highly accurate outputs on account of harnessing the insights of informed minorities. Moreover, some of the major challenges faced by such markets---relating to illiquidity, manipulation, and barriers to entry---seem surmountable in light of the empirical evidence. Consequently, information markets should be of interest to applied epistemologists concerned with how to improve our social, epistemic practices. In fact, there are a number of promising, practical applications of information markets that speak directly to the nature, value, and methods of applied epistemology, and also raise a number of significant questions in epistemology and political philosophy. While the field of information market research is still new, and the philosophical study of them newer still, the present overview should make it clear that information markets warrant further investigation by applied philosophers.},
	author = {Ahlstrom-Vij, Kristoffer},
	booktitle = {A Companion to Applied Philosophy},
	chapter = {7},
	date-added = {2021-01-01 22:51:49 +0000},
