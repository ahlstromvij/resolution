@inproceedings{luckneretal2008,
	abstract = {Accurate forecasts are essential in many areas such as business and sports forecasting. Prediction markets are a promising approach for forecasting future events and are increasingly used to aggregate information on particular future events of interest such as elections, sports events, and Oscar winners. In this paper, we present the results of an empirical study that compares the forecast accuracy of a prediction market for the FIFA World Cup 2006 to predictions derived from the FIFA world ranking and to a random predictor. We find that prediction markets for the FIFA World Cup outperform predictions based on the FIFA world ranking as well as the random predictor in terms of forecast accuracy.},
