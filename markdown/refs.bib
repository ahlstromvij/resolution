@article{ROSS1977279,
	abstract = {Evidence from four studies demonstrates that social observers tend to perceive a ``false consensus'' with respect to the relative commonness of their own responses. A related bias was shown to exist in the observers' social inferences. Thus, raters estimated particular responses to be relatively common and relatively unrevealing concerning the actors' distinguishing personal dispositions when the responses in question were similar to the raters' own responses; responses differing from those of the rater, by contrast, were perceived to be relatively uncommon and revealing of the actor. These results were obtained both in questionnaire studies presenting subjects with hypothetical situations and choices and in authentic conflict situations. The implications of these findings for our understanding of social perception phenomena and for our analysis of the divergent perceptions of actors and observers are discussed. Finally, cognitive and perceptual mechanisms are proposed which might account for distortions in perceived consensus and for corresponding biases in social inference and attributional processes.},
	author = {Ross, Lee and Greene, David and House, Pamela},
	date-added = {2021-01-01 22:47:33 +0000},
	date-modified = {2021-01-04 23:29:57 +0000},
	doi = {https://doi.org/10.1016/0022-1031(77)90049-X},
	issn = {0022-1031},
	journal = {Journal of Experimental Social Psychology},
	number = {3},
	pages = {279 - 301},
	title = {The 'false consensus effect': An egocentric bias in social perception and attribution processes},
	url = {http://www.sciencedirect.com/science/article/pii/002210317790049X},
	volume = {13},
	year = {1977},
	bdsk-url-1 = {http://www.sciencedirect.com/science/article/pii/002210317790049X},
	bdsk-url-2 = {https://doi.org/10.1016/0022-1031(77)90049-X}}

@article{bergetal2008,
	abstract = {"Prediction markets" are designed specifically to forecast events such as elections. Though election prediction markets have been being conducted for almost twenty years, to date nearly all of the evidence on efficiency compares election eve forecasts with final pre-election polls and actual outcomes. Here, we present evidence that prediction markets outperform polls for longer horizons. We gather national polls for the 1988 through 2004 U.S. Presidential elections and ask whether either the poll or a contemporaneous Iowa Electronic Markets vote-share market prediction is closer to the eventual outcome for the two-major-party vote split. We compare market predictions to 964 polls over the five Presidential elections since 1988. The market is closer to the eventual outcome 74% of the time. Further, the market significantly outperforms the polls in every election when forecasting more than 100{\^A} days in advance.},
	author = {Berg, Joyce E. and Nelson, Forrest D. and Rietz, Thomas A.},
