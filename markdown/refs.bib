	isbn = {9781118869109},
	keywords = {information markets, prediction markets, social epistemology, information aggregation, preference aggregation, wisdom of crowds},
	pages = {89-102},
	publisher = {John Wiley & Sons, Ltd},
	title = {Information Markets},
