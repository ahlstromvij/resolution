@article{Galesic:2018aa,
	abstract = {Election outcomes can be difficult to predict. A recent example is the 2016 US presidential election, in which Hillary Clinton lost five states that had been predicted to go for her, and with them the White House. Most election polls ask people about their own voting intentions: whether they will vote and, if so, for which candidate. We show that, compared with own-intention questions, social-circle questions that ask participants about the voting intentions of their social contacts improved predictions of voting in the 2016 US and 2017 French presidential elections. Responses to social-circle questions predicted election outcomes on national, state and individual levels, helped to explain last-minute changes in people's voting intentions and provided information about the dynamics of echo chambers among supporters of different candidates.},
	author = {Galesic, M., and Bruine de Bruin, W., and Dumas, M., and Kapteyn, A., and Darling, J. E., and Meijer, E.},
	da = {2018/03/01},
